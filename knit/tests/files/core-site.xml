  </property>

  <property>
    <name>fs.tachyon.impl</name>
    <value>tachyon.hadoop.TFS</value>
  </property>

  <property>
     <name>hadoop.proxyuser.httpfs.hosts</name>
     <value>*</value>
  </property>

</configuration>
